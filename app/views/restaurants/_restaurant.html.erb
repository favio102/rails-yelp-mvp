<div class="container">
  <div id="<%= dom_id restaurant %>">
    <h1>
      <%= restaurant.name %>
    </h1>

    <p>
      <strong>Address:</strong>
      <%= restaurant.address %>
    </p>

    <p>
      <strong>Phone number:</strong>
      <%= restaurant.phone_number %>
    </p>

    <p>
      <strong>Category:</strong>
      <%= restaurant.category %>
    </p>

  </div>
</div>
